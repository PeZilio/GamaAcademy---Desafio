{
    "openapi": "3.0.0",

    "info": {
        "tittle": "API Desafio",
        "description": "api do desafio",
        "version":"0.0.1"
},

"servers":[
{
    "url": "http://localhost:5000",
    "description": "ambiente de desenvolvimento"
}
],

"paths":{
    "/register": {
        "post": {
            "summary": "Cadastro de candidato",
            "decription": "Rota de cadastro de usuário, recebe: nome, email, gênero, cep e demais informações",
            "tags": ["candidato"],
            "requestBody":{
                "content": {
                    "application/json": {
                        "schema": {
                            "properties": {
                                "nomecompelto": {
                                    "type": "string"
                                },
                                "datanasc": {
                                    "type": "date"
                                },
                                "genero": {
                                    "type": "string"
                                },
                                "estcivil":{
                                    "type": "string"
                                },
                                "identidade":{
                                    "type": "string"
                                },
                                "cpf":{
                                    "type": "string"
                                },
                                "tipohabilitacao":{
                                    "type": "string"
                                },
                                "veiculo":{
                                    "type": "string"
                                },
                                "rua":{
                                    "type": "string"
                                },
                                "numero":{
                                    "type": "string"
                                },
                                "complemento":{
                                    "type": "string"
                                },
                                "bairro":{
                                    "type": "string"
                                },
                                "cidade":{
                                    "type": "string"
                                },
                                "uf":{
                                    "type": "string"
                                },
                                "telfixo1":{
                                    "type": "string"
                                },
                                "telfixo2":{
                                    "type": "string"
                                },
                                "telcontato":{
                                    "type": "string"
                                },
                                "nomecontato":{
                                    "type": "string"
                                },
                                "telcelular":{
                                    "type": "string"
                                },
                                "email":{
                                    "type": "string"
                                },
                                "linkedin":{
                                    "type": "string"
                                },
                                "skype":{
                                    "type": "string"
                                },
                                "cargo":{
                                    "type": "string"
                                }
                            }
                        }
                    }   
                }
            }
        }
    }
}
}


